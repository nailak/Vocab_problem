{
  "name": "IO Lab Project 2",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "browser_action": {
    "default_icon": "icon.png",
	"default_popup": "popup.html"
  },
  "permissions": [
    "tabs", "http://www.quora.com/*"
  ],
  "background" : {
	"scripts" : ["background.js"]
  },
  "content_scripts" : [
    {
      "matches" : [ "http://*/*" ],
      "js" : [ "contentscript.js" ],
      "run_at" : "document_idle",
      "all_frames" : false
    }
  ]
}

